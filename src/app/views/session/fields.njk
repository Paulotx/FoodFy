<div class="item">
    <div>Email</div>
        <input 
            type="text"
            name="email"
            onblur="ValidateLogin.apply(this, 'isEmail')"
            value="{{ user.email }}"
            placeholder="Digite o email"
        >
</div>

<div class="item">
    <div>Senha</div>
        <input 
            type="password"
            name="password"
            placeholder="Digite sua senha"
        >
</div>